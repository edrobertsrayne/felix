{
  "name": "felix",
  "version": "1.0.0",
  "type": "module",
  "bin": {
    "felix": "./dist/cli.js"
  },
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/cli.ts --outdir dist --target node --external react-devtools-core",
    "build:all": "bun build src/cli.ts --outdir dist --target node --external react-devtools-core && bun build src/gateway-service.ts --outdir dist --target node --external react-devtools-core && bun build src/index.ts --outdir dist --target node --external react-devtools-core",
    "start": "bun run src/index.ts",
    "gateway": "bun run src/cli.ts gateway",
    "gateway:start": "bun run src/cli.ts gateway start",
    "gateway:stop": "bun run src/cli.ts gateway stop",
    "tui": "bun run src/cli.ts tui"
  },
  "dependencies": {
    "@inkjs/ui": "^2.0.0",
    "@openrouter/sdk": "^0.8.0",
    "commander": "^14.0.3",
    "dotenv": "^16.4.0",
    "ink": "^6.7.0",
    "marked": "^17.0.3",
    "node-telegram-bot-api": "^0.67.0",
    "openai": "^4.77.0",
    "react": "^19.2.4",
    "tiktoken": "^1.0.22",
    "ws": "^8.18.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/node-telegram-bot-api": "^0.64.13",
    "@types/react": "^19.2.14",
    "@types/ws": "^8.5.10",
    "bun-types": "^1.3.9",
    "eslint": "^10.0.0",
    "prettier": "^3.8.1",
    "react-devtools-core": "^7.0.1",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.56.0"
  }
}
